{
  "name": "remote-work-queue",
  "version": "0.0.0",
  "description": "a Node.js work queue distributing tasks via RabbitMQ",
  "main": "client.js",
  "scripts": {
    "test": "mocha --reporter spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fwdcp/remote-work-queue.git"
  },
  "author": "Forward Command Post (http://fwdcp.net)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fwdcp/remote-work-queue/issues"
  },
  "homepage": "https://github.com/fwdcp/remote-work-queue#readme",
  "dependencies": {
    "amqplib": "^0.4.2",
    "async-class": "^0.4.1",
    "lodash": "^4.14.2",
    "moment": "^2.14.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "co": "^4.6.0",
    "eslint": "^3.2.2",
    "eslint-plugin-lodash": "^1.10.1",
    "mocha": "^3.0.2"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:lodash/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 6
    },
    "plugins": [
      "lodash"
    ],
    "rules": {
      "array-callback-return": "error",
      "arrow-body-style": "error",
      "arrow-parens": [
        "error",
        "as-needed"
      ],
      "arrow-spacing": "error",
      "block-scoped-var": "error",
      "brace-style": [
        "error",
        "stroustrup"
      ],
      "callback-return": "error",
      "comma-style": "error",
      "curly": "error",
      "dot-location": "error",
      "dot-notation": "error",
      "eqeqeq": "error",
      "global-require": "error",
      "handle-callback-err": "error",
      "key-spacing": "error",
      "no-confusing-arrow": [
        "error",
        {
          "allowParens": true
        }
      ],
      "no-mixed-requires": "error",
      "no-new-require": "error",
      "no-path-concat": "error",
      "no-process-exit": "error",
      "no-shadow": "error",
      "no-throw-literal": "error",
      "no-unneeded-ternary": "error",
      "no-use-before-define": "error",
      "object-shorthand": "error",
      "operator-assignment": "error",
      "prefer-spread": "error",
      "prefer-template": "error",
      "require-yield": "error",
      "quotes": [
        "error",
        "single"
      ],
      "template-curly-spacing": "error",
      "yoda": "error"
    }
  }
}
